// Declarative Pipelines
Pipeline{
	agent any
	stages {
		stage('Build'){
			steps {
				echo "Build"
			}
		}
		stage('Test'){
			steps {
				echo "Test"
			}
		}
		stage('Int Test'){
			steps {
				echo "Int Test"
			}
		}
	}
}